{
  "id": "icon",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Icon",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "top": {
      "$ref": "types/percentage.schema.json#percentage"
    },
    "left": {
      "$ref": "types/percentage.schema.json#percentage"
    },
    "size": {
      "type": "number",
      "minimum": 0,
      "maximum": 500
    },
    "rotate": {
      "$ref": "types/degree.schema.json#degree"
    },
    "emoji": {
      "type": "string"
    }
  }
}
